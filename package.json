{
  "name": "advanced-react",
  "version": "1.0.0",
  "main": "lib/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node --name appDev",
    "reload": "pm2 reload all",
    "webpack": "webpack -wd",
    "test": "jest --watch",
    "verify-tests": "jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "prod": "NODE_ENV=production NODE_PATH=./build pm2 start build/server.js -i max --name appProd"

  },
  "jest": {
    "moduleNameMapper": {
      "^Components(.*)$": "<rootDir>/lib/components/$1",
      "^Renderers(.*)$": "<rootDir>/lib/renderers/$1"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.21.2",
    "jest": "^24.9.0",
    "react-test-renderer": "^16.8.6",
    "webpack-cli": "^3.3.0",
    "why-did-you-update": "^1.0.6"
  },
  "dependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "core-js": "2.6.5",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "lodash.debounce": "^4.0.8",
    "lodash.pickby": "^4.6.0",
    "pm2": "^3.4.1",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.8.6",
    "webpack": "^4.29.6"
  }
}
